    key <AC10> {        [ oslash,        Ooblique,     semicolon,    colon      ]        };
    key <AC11> {        [ ae,            AE,           apostrophe,   quotedbl   ]        };

    key <LSGT> {        [ grave,         asciitilde,   dead_grave,   dead_tilde ]        };

    include "level3(ralt_switch)"
};
